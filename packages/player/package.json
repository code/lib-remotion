{
	"name": "@remotion/player",
	"version": "4.0.126",
	"description": "Remotion Player",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.mjs",
	"sideEffects": false,
	"scripts": {
		"formatting": "prettier src --check",
		"lint": "eslint src --ext ts,tsx",
		"build": "node ensure-correct-version.js && rollup --config rollup.config.js && tsc -d",
		"watch": "tsc -w",
		"prerelease": "cp ../../README.md ."
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/cjs/index.d.ts",
			"module": "./dist/esm/index.mjs",
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"author": "Jonny Burger <jonny@remotion.dev>",
	"maintainers": [
		"Jonny Burger <jonny@remotion.dev>",
		"Shankhadeep Dey <shankhadeepdey99@gmail.com>"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"dependencies": {
		"remotion": "workspace:*"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.276",
		"@rollup/plugin-typescript": "^8.2.0",
		"@testing-library/react": "13.1.1",
		"@types/node": "18.14.6",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"csstype": "^3.1.1",
		"eslint": "8.42.0",
		"eslint-plugin-require-extensions": "^0.1.2",
		"jsdom": "20.0.1",
		"prettier": "3.1.1",
		"prettier-plugin-organize-imports": "3.2.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rollup": "^2.70.1",
		"rollup-plugin-banner2": "^1.2.2",
		"vitest": "0.31.1",
		"webpack": "5.83.1",
		"zod": "^3.22.3"
	},
	"keywords": [
		"remotion",
		"ffmpeg",
		"video",
		"react",
		"player"
	],
	"publishConfig": {
		"access": "public"
	}
}
