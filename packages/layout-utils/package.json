{
	"name": "@remotion/layout-utils",
	"version": "4.0.124",
	"description": "Layout Utils for Remotion",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.mjs",
	"sideEffects": false,
	"scripts": {
		"formatting": "prettier src --check",
		"lint": "eslint src --ext ts,tsx",
		"watch": "tsc -w",
		"build": "rollup --config rollup.config.js && tsc -d"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/cjs/index.d.ts",
			"module": "./dist/esm/index.mjs",
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"author": "Yehor Misiats (https://github.com/satelllte)",
	"maintainers": [
		"Jonny Burger <jonny@remotion.dev>",
		"Yehor Misiats (https://github.com/satelllte)"
	],
	"contributors": [],
	"license": "MIT",
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"dependencies": {},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.276",
		"@rollup/plugin-typescript": "^8.2.0",
		"@types/node": "18.14.6",
		"eslint": "8.42.0",
		"prettier": "3.1.1",
		"prettier-plugin-organize-imports": "3.2.4",
		"rollup": "^2.70.1"
	},
	"keywords": [
		"remotion",
		"layout-utils"
	],
	"publishConfig": {
		"access": "public"
	}
}
